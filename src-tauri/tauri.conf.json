{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "bun run build:web",
    "beforeDevCommand": "bun run dev:web",
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173"
  },
  "bundle": {
    "active": true,
    "category": "Game",
    "copyright": "",
    "targets": [
      "deb",
      "appimage"
    ],
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "webviewInstallMode": {
        "silent": true,
        "type": "embedBootstrapper"
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "This is a fork of OpenTG, the free and open source version of a business simulation tycoon game.",
    "linux": {
      "deb": {
        "depends": [
          "libwebkit2gtk-4.0-37"
        ]
      }
    }
  },
  "productName": "OpenTG",
  "mainBinaryName": "OpenTG",
  "version": "../package.json",
  "identifier": "open.source",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": true,
        "height": 600,
        "resizable": true,
        "title": "OpenTG",
        "width": 800,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}